<% provide(:title, "Edit") %>

<div class="container">
  <h1>アカウント情報の更新</h1>
　　<div class="col-md-6 col-md-offset-3">
　　  <%= form_with(url: edit_user_path, model: @user, local: true) do |f| %>
　　    <%= f.label :name, "名前" %>
　　    <%= f.text_field :name, class: "form-control", placeholder: "Name" %>
　　    
　　    <%= f.label :email, "メールアドレス" %>
　　    <%= f.email_field :email, class: "form-control", placeholder: "Email" %>
　　    
　　    <%= f.label :, "所属" %>
　　    <%= f.text_field :email, class: "form-control", placeholder: "Email" %>
　　    
　　    <%= f.label :password, "所属" %>
　　    <%= f.password_field :email, class: "form-control", placeholder: "Email" %>
　　    
　　    <%= f.label :password-confirmation, "所属" %>
　　    <%= f.text_field :email, class: "form-control", placeholder: "Email" %>
　　</div>
</div>